<div class="container">


  <h1 class="font-weight-light text-center text-lg-left mt-4 mb-0">Thumbnail Gallery</h1>

  <hr class="mt-2 mb-5">

  <div class="row text-center text-lg-left align-items-center">

  <%for(let i = 1; i < 17; i++){ %>
    <div class="col-lg-3 col-md-4 col-6"  >
          <a  href="/images/Gallery/<%=i + '.jpg' %>" data-lightbox="image-1" data-title="My caption">
                <img  width="100%" height="100%" src="/images/Gallery/<%=i + '.jpg' %>" alt=""  >
              </a>
    </div>
  <% } %>
   
  
  </div>

</div>

